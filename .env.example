# AO Plugin Configuration
# Copy this file to .env and fill in your values

# API Key for AO (optional)
AO_API_KEY=your_api_key_here

# Gateway URL for Arweave (default: https://arweave.net)
AO_GATEWAY_URL=https://arweave.net

# GraphQL URL for Arweave (default: https://arweave.net/graphql)
AO_GRAPHQL_URL=https://arweave.net/graphql

# Message Unit URL (optional)
AO_MU_URL=https://mu-1.ao-testnet.xyz

# Compute Unit URL (optional)
AO_CU_URL=https://cu-1.ao-testnet.xyz

# Connection mode for aoconnect (default: legacy)
# Options: legacy, mainnet
AO_MODE=legacy

# Default timeout for operations in milliseconds (default: 30000)
AO_DEFAULT_TIMEOUT=30000
